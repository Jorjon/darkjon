<?xml version="1.0" encoding="utf-8"?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<languages>
		<language name="properties">
			<lexer name="properties" style-bits="7" />
			<file-extensions>properties,ini,yaml</file-extensions>
			<!-- COLORING_START -->
			<editor-style caret-fore="0x000000" caretline-back="0xe0e0e0" selection-fore="0xffffff" selection-back="0x3399ff" margin-fore="0x2F393C" margin-back="0x2F393C" />
			<use-styles>
				<style name="default" fore="0xE0E2E4" back="0x293134" size="9" font="Courier New" />
				<style name="comment" fore="0x7D8C93" />
				<style name="section" fore="0xffffff" back="0x255B83" eolfilled="true" />
				<style name="assignment" fore="0xEC7600" />
				<style name="defval" />
				<style name="key" fore="0x93C763" />
				<style name="gdefault" fore="0x81969A" />
				<style name="linenumber" fore="0x81969A" />
				<style name="bracelight" fore="0x72898D" back="0xe2F393C" bold="true" />
				<style name="bracebad" bold="true" />
				<style name="controlchar" fore="0xffffff" />
				<style name="indentguide" fore="0xc0c0c0" />
				<style name="lastpredefined" fore="0x666666" />
			</use-styles>
			<!-- COLORING_END -->
		</language>
	</languages>
</Scintilla>